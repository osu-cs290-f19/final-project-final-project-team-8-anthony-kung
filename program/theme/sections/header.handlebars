<center>
  <h1>
    {{#each site}}
      {{#if headerLogo}}
        <img class="headerLogo" src="{{logo}}" width="{{logoWidth}}" height="{{logoHeight}}" />
      {{/if}}
      <br />
      {{#if headerTitle}}
        {{title}}
      {{/if}}
    {{/each}}
  </h1>
  <div id="navbar">
    {{#each navItems}}
      {{> navItem}}
    {{/each}}
  </div>
</center>
